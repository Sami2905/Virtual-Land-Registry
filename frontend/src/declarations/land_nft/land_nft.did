type LandMetadata = record {
  name: text;
  coordinates: text;
  size: text;
  media_hash: text;
};

type Listing = record {
  id: nat64;
  price: nat64;
};

service : {
  mint : (LandMetadata) -> (nat64);
  get_owner : (nat64) -> (principal);
  get_metadata : (nat64) -> (LandMetadata);
  transfer : (nat64, principal) -> (bool);
  list_land : (nat64, nat64) -> ();
  get_listings : () -> (vec Listing);
  buy_land : (nat64) -> ();
};
